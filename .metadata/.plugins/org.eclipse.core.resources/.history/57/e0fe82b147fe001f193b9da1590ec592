public class Ex01 {

    public static void main(String[] args) {
        // Ex01.java
        
        //Jsoup
        //- 자바 프로그램

        try {
            
            String url = "https://jsoup.org/";
            
            Document doc = Jsoup.connect(url).get();
            
        } catch (Exception e) {
            e.printStackTrace();
        }
    }

}
