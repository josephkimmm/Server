<head>
	<meta charset="UTF-8">
	<title>Insert title here</title>
</head>
<body>
	<!-- ex04.jsp -->
	<%
		//자바 영역 > 스크립틀릿
		int dan = 5;
	%>
	
	<%for (int i=0;i<=9; i++) { %>
			<div><%= dan %> x <%= i %> = <%= dan * i %></div>
			<div><%= dan + " x " + i + " = " + (dan * i) %></div>
	<% } %>
	
	<hr>
	
	<div>단: <%= dan %></div>
</body>
